{
  "name": "vue-async-component-provider",
  "version": "0.4.0",
  "description": "Customizable Suspense replacement. Use it as a wrapper component to manage state of multiple async dependencies in a component tree.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "npm run build"
  },
  "module": "./dist/async-component-provider.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/async-component-provider.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vue",
    "suspense",
    "async",
    "component",
    "provider"
  ],
  "author": "AntaraSI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antarasi/vue-async-component-provider.git"
  },
  "bugs": {
    "url": "https://github.com/antarasi/vue-async-component-provider/issues"
  },
  "homepage": "https://github.com/antarasi/vue-async-component-provider#readme",
  "peerDependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^2.2.10"
  }
}
